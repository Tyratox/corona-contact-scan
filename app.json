{
  "expo": {
    "name": "Ciao SCAN",
    "slug": "ciao-scan",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.3",
    "orientation": "portrait",
    "primaryColor": "#FFEA2A",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFEA2A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "locales": {
      "de": "./locales/ios/de.json",
      "fr": "./locales/ios/fr.json",
      "it": "./locales/ios/it.json"
    },
    "ios": {
      "bundleIdentifier": "ch.feuerschutz.ciao.scan",
      "buildNumber": "1.0.3.1",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "The app needs access to your camera in order to scan qr codes on other devices."
      }
    },
    "android": {
      "package": "ch.feuerschutz.ciao.scan",
      "versionCode": 4,
      "permissions": ["CAMERA"]
    },
    "description": ""
  }
}
